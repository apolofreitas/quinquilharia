{
  "name": "postgres-drizzle",
  "license": "MIT",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "yarn db:migrate",
    "db:generate": "drizzle-kit generate:pg --config drizzle/drizzle.config.ts",
    "db:migrate": "tsx drizzle/migrate.ts",
    "db:up": "drizzle-kit up:pg --config drizzle/drizzle.config.ts",
    "db:drop": "drizzle-kit drop --config drizzle/drizzle.config.ts",
    "db:check": "drizzle-kit check:pg --config drizzle/drizzle.config.ts",
    "db:studio": "ddrizzle-kit studio --port 3000 --verbose --config drizzle/drizzle.config.ts"
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@types/node": "20.11.17",
    "@types/react": "18.2.55",
    "@types/react-dom": "18.2.19",
    "@vercel/postgres": "0.7.2",
    "autoprefixer": "10.4.17",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "drizzle-kit": "^0.20.14",
    "drizzle-orm": "^0.29.3",
    "eslint": "8.56.0",
    "eslint-config-next": "14.1.0",
    "next": "14.1.0",
    "next-themes": "^0.2.1",
    "pg": "^8.11.3",
    "postcss": "8.4.35",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.3.3"
  },
  "devDependencies": {
    "dotenv": "^16.4.2",
    "tsx": "^4.7.1"
  }
}
